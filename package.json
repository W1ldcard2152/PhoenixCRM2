{
  "name": "auto-repair-crm",
  "version": "1.0.0",
  "description": "CRM system for auto repair shop",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:server": "nodemon server.js",
    "dev:client": "cd src/client && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd src/client && npm run build",
    "prod": "npm run build && npm start",
    "install:client": "cd src/client && npm install",
    "install:all": "npm install && npm run install:client",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1350.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-xss-sanitizer": "^2.0.0",
    "formik": "^2.4.6",
    "helmet": "^6.1.5",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.0",
    "jspdf": "^3.0.1",
    "moment": "^2.30.1",
    "mongodb": "^6.16.0",
    "mongoose": "^7.0.3",
    "mongosh": "^2.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react-router-dom": "^7.6.0",
    "react-scripts": "^5.0.1",
    "twilio": "^4.10.0",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
