{
  "name": "auto-repair-crm",
  "version": "1.0.0",
  "description": "CRM system for auto repair shop",
  "main": "server.js",
  "scripts": {
    "start": "node src/server/server.js",
    "server": "nodemon src/server/server.js",
    "client": "cd src/client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd src/client && npm run build",
    "install-client": "cd src/client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1350.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-xss-sanitizer": "^2.0.0",
    "formik": "^2.4.6",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "mongosh": "^2.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react-router-dom": "^6.21.3",
    "react-scripts": "^5.0.1",
    "twilio": "^4.10.0",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}